 .container
  .row
    .col-12
      .breadcrumbs
        ul.flex.flex-wrap.align-items-center.p-0.m-0
          li
            a(href='#')
              i.fa.fa-home
              |  Home
          li Contact
      // .breadcrumbs
    // .col
